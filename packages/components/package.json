{
  "name": "@carto/airship-components",
  "version": "0.0.0",
  "description": "CARTO Airship components",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "dev": "stencil build --dev --watch --serve",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "echo 'No linter available yet.'",
    "serve": "serve -p 5001"
  },
  "devDependencies": {
    "@stencil/core": "^0.10.0",
    "@types/jest": "^21.1.1",
    "@stencil/sass": "0.0.5",
    "jest": "^21.2.1",
    "lerna": "^2.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CartoDB/airship.git"
  },
  "author": "CARTO Dev Team",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/CartoDB/airship/issues"
  },
  "homepage": "https://github.com/CartoDB/airship",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
